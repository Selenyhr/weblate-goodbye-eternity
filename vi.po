msgctxt "yasuka_love_sexe_repeat_79977168"
msgid "M-"
msgstr "Đị-"

#: game/story/home_player_noriko_room.dialogue
msgctxt "menu_rejet_noriko_final_8f3ab287"
msgid "M-"
msgstr "Vã-"

#: game/story/home_player_noriko_room.dialogue
msgctxt "talk_noriko_8f3ab287"
msgid "M-"
msgstr "Loằ-"

#: game/story/restaurant_sakuya_room.dialogue
msgctxt "restaurant_sakuya_room_19fff193"
msgid "M-"
msgstr "Đ-"

#: game/story/park.dialogue
msgctxt "park_d69b7ea7"
msgid "M-"
msgstr "Như-"

#: game/story/home_minato_exterior.dialogue
msgctxt "talk_minato_8f3ab287"
msgid "M-"
msgstr "Nhưn-"

#: game/story/home_player_corridor.dialogue
msgctxt "home_player_corridor_3b13464d"
msgid "Bon sang, c'était quoi ça ?"
msgstr "Vãi chưởng thật, chuyện gì vừa xảy ra vậy ?"

#: game/story/home_player_corridor.dialogue
msgctxt "home_player_corridor_d4589c13"
msgid "Merde {{noriko.n}}, pourquoi tu ne veux rien me dire ?"
msgstr "{{noriko.n}} ơi, sao bây đã không thưa anh một tiếng chứ ?"

#: game/story/home_player_corridor.dialogue
msgctxt "home_player_corridor_7452d276"
msgid "Quel putain d'idiot..."
msgstr "Mình quả thật là một thằng óc bò..."

#: game/story/home_player_corridor.dialogue
msgctxt "home_player_corridor_954a7335"
msgid "Je reproche à {{noriko.n}} ce que je faisais depuis le début."
msgstr "Mình cứ đổ cho {{noriko.n}} mọi rắc rối mà chính tay mình gây ra từ trước tới nay."

#: game/story/home_player_corridor.dialogue
msgctxt "home_player_corridor_0edb3cad"
msgid "En pensant pouvoir gérer mes problèmes moi-même je me rends compte que je n'ai fait qu'inquiéter ceux qui tenaient à moi."
msgstr "Cứ ra dáng làm anh húng, mình nhận ra, mình chỉ khiến cho những người mình thương và thương mình phải lo lắng."

#: game/story/home_player_noriko_room.dialogue
msgctxt "menu_rejet_noriko_final_db51f401"
msgid "En pensant pouvoir gérer mes problèmes moi-même je me rends compte que je n'ai fait qu'inquiéter ceux qui tenaient à moi."
msgstr "Mình tưởng mình có thể giải quyết đớn đau của gia đình, nhưng ngược lại mình chỉ khiến những người thân yêu thêm sầu khổ"

#: game/story/home_player_corridor.dialogue
msgctxt "home_player_corridor_d9539ee1"
msgid "J'étais vraiment à côté de la plaque."
msgstr "Thật sự, mình đã đi quá xa rồi."

#: game/story/home_player_noriko_room.dialogue
msgctxt "menu_rejet_noriko_final_9f91fa65"
msgid "J'étais vraiment à côté de la plaque."
msgstr "Mình thật sự đã đi xa quá rồi..."

#: game/story/home_player_corridor.dialogue
msgctxt "home_player_corridor_528e73bd"
msgid "{{noriko.n}} peu importe ce qui s'est passé, tu pourras compter sur moi à partir de maintenant !"
msgstr "{{noriko.n}}, kể từ giờ phút này, dẫu sông có cạn, núi có dời, hãy cứ tin tưởng vào anh !"

#: game/story/home_player_corridor.dialogue
msgctxt "t90f5acffaa"
msgid "Je peux savoir où tu vas ?"
msgstr "... Tao... Em có thể biết anh đi đâu không ?"

#: game/story/home_player_corridor.dialogue
msgctxt "t6b0817d6d5"
msgid "... Je ne vois pas en quoi ça te regarde."
msgstr "... Chẳng liên quan gì tới mày sất."

#: game/story/home_player_corridor.dialogue
msgctxt "t6c4376aab6"
msgid "Ça me concerne depuis que tu passes ton temps à t'attirer des problèmes."
msgstr "Tao hỏi vì mỗi ngày mày lại có hứng đi tìm một đống cứt mới để chõ mũi vào."

#: game/story/home_player_corridor.dialogue
msgctxt "t5392c14ef6"
msgid "Je vais au dojo..."
msgstr "Tao đi tới Võ Đường..."

#: game/story/home_player_corridor.dialogue
msgctxt "t123c0249c6"
msgid "Non non, \"nous\" allons au dojo !"
msgstr "Không không, \"chúng ta \" sẽ đi tới Võ Đường !"

#: game/story/home_player_corridor.dialogue
msgctxt "tc6ccb69884"
msgid "Quoi ?! Non, je peux me débrouiller sans toi."
msgstr "Đéo ?! Đéo nhá, tao tự giữ mạng mình được rồi."

#: game/story/home_player_corridor.dialogue
msgctxt "t99357b3de5"
msgid "Je ne me souviens pas t'avoir demandé ton avis."
msgstr "Ai hỏi ? Mày ý kiến ý cò làm gì ?"

#: game/story/home_player_corridor.dialogue
msgctxt "t746f84d886"
msgid "Peu importe ce que tu peux penser, je viens avec toi cette fois-ci."
msgstr "Mày muốn gì kệ xác mày, lần này tao sẽ đi với mày."

#: game/story/home_player_corridor.dialogue
msgctxt "tbabc5e0a15"
msgid "Que tu le veuilles ou non."
msgstr "Dù mày có muốn hay không."

#: game/story/home_player_corridor.dialogue
msgctxt "t79d41d1b91"
msgid "...[w] Bon après tout, si ça peut te faire plaisir."
msgstr "...[w] Ờ, nếu mày thích thì."

#: game/story/home_player_corridor.dialogue
msgctxt "tff74b889f6"
msgid "Parfait !"
msgstr "Đấy có phải ngoan không !"

#: game/story/home_player_saito_room.dialogue
msgctxt "PARFAIT_90F307"
msgid "Parfait !"
msgstr ""

#: game/story/home_player_livingroom.dialogue
msgctxt "home_player_livingroom_f67a45a6"
msgid "Parfait !"
msgstr "Perfecto !"

#: game/story/warehouse_interior.dialogue
msgctxt "tce302fcad6"
msgid "Parfait !"
msgstr "Tuyệt !"

#: game/story/onsen_interior.dialogue
msgctxt "t31937da5c5"
msgid "Parfait !"
msgstr "Tuyệt !"

#: game/story/home_asami_exterior.dialogue
msgctxt "tba2629200b"
msgid "Parfait !"
msgstr "Tuyệt !"

#: game/story/konbini_exterior.dialogue
msgctxt "konbini_exterior_aa21a1f9"
msgid "Parfait !"
msgstr "MĐược thế thì tốt !"

#: game/story/home_player_corridor.dialogue
msgctxt "home_player_corridor_a3e8e4fc"
msgid "Putain à quel point je suis tombé bas pour qu'on en vienne à se prendre la tête pour m'aider ?"
msgstr "Mình kém cỏi, cặn bã tới nhường nào để bọn họ phải châu đầu vào lắng lo thay cho mình ?"

#: game/story/home_player_corridor.dialogue
msgctxt "home_player_corridor_5d0656e5"
msgid "Je répète les mêmes schemas que ma précédente vie... Je n'arrive pas à m'affirmer correctement."
msgstr "Mình đang dần men theo chính lối mòn cũ ở tiền kiếp... Lời mình nói chẳng có tí trọng lượng, ai cũng phải lo lót cho mình."

#: game/story/home_player_corridor.dialogue
msgctxt "home_player_corridor_90362c2d"
msgid "{{saito.n}} attend !"
msgstr "{{saito.n}} dừng bước người lạ ơi !"

#: game/story/home_player_corridor.dialogue
msgctxt "home_player_corridor_b1163c93"
msgid "Je suis vraiment désolée... Je ne voulais pas m'emporter contre {{noriko.n}}."
msgstr "Tui xin lỗi... Tui lỡ to tiếng với {{noriko.n}}"

#: game/story/home_player_corridor.dialogue
msgctxt "home_player_corridor_048c1b14"
msgid "Mais quand j'ai appris ce qu'elle t'avais forcé à faire.... ça m'a mise hors de moi."
msgstr "Nhưng mà biết những gì mà con bé bắt ông kinh qua... Tôi tức phát điên ý."

#: game/story/home_player_corridor.dialogue
msgctxt "home_player_corridor_27498703"
msgid "Ne t'inquiète pas, je ne t'en veux pas du tout."
msgstr "Đừng quá lo lắng mà bà, tôi không có trách bà đâu."

#: game/story/home_player_corridor.dialogue
msgctxt "home_player_corridor_9839dba9"
msgid "{{noriko.n}} n'est pas quelqu'un qui sait faire preuve de tact."
msgstr "{{noriko.n}} nó không có phải là một đứa thông thái cho lắm."

#: game/story/home_player_corridor.dialogue
msgctxt "home_player_corridor_59d70329"
msgid "Elle passera vite à autre chose comme à son habitude dès que ça la lassera."
msgstr "Rồi con bé cũng chán mà bỏ đi làm việc khác thôi mà."

#: game/story/home_player_corridor.dialogue
msgctxt "home_player_corridor_a4fc4062"
msgid "Hummm je n'en suis pas certaine... Il semble vraiment que cette situation l'angoisse beaucoup."
msgstr "Tui... không biết nữa... Có vẻ như mấy chuyện này đang khiến con bé stress lắm."

#: game/story/home_player_corridor.dialogue
msgctxt "home_player_corridor_cae88a0c"
msgid "Je pense que tu devrais sincèrement avoir une discussion avec elle."
msgstr "Thiết nghĩ là ông nên có một buổi đối thoạt với em nó mà làm rõ quan điểm của ông ấy."

#: game/story/home_player_corridor.dialogue
msgctxt "home_player_corridor_15183c2b"
msgid "Je ne suis pas sûr que..."
msgstr "Tôi không chắc nữa, nhưng sẽ cố gắng..."

#: game/story/home_player_corridor.dialogue
msgctxt "home_player_corridor_88cb685b"
msgid "Mon chéri, fais moi confiance !"
msgstr "Cậu yêu, tin tui đi !"

#: game/story/home_player_corridor.dialogue
msgctxt "home_player_corridor_bf8d799b"
msgid "{{saito.n}}, fais-moi confiance !"
msgstr "{{saito.n}}, cứ tin ở tui !"

#: game/story/home_player_corridor.dialogue
msgctxt "home_player_corridor_e8ae8d31"
msgid "...[w] Très bien, je vais aller la voir mais ne t'attends pas à un miracle non plus."
msgstr "...[w] Thôi được, vậy tôi sẽ đi chuyện trò với con bé. Không dám đảm bảo thành công nha."

#: game/story/home_player_corridor.dialogue
msgctxt "home_player_corridor_2420781b"
msgid "Ce sera toujours mieux que rien."
msgstr "Có thể vẫn hơn không."

#: game/story/home_player_corridor.dialogue
msgctxt "home_player_corridor_419ee45c"
msgid "...Alors ?"
msgstr "...Thế nào rồi ?"

#: game/story/home_player_corridor.dialogue
msgctxt "home_player_corridor_d69513dd"
msgid "C'est bon, il est temps d'aller s'occuper de ces trois branleurs !"
msgstr "Đả thông tư tưởng rồi, đi làm gỏi maasyy đứa âm binh kia thôi !"

#: game/story/home_player_corridor.dialogue
msgctxt "home_player_corridor_ebe35646"
msgid "Parfait, j'avais peur que tu te defiles mais je suis contente de m'être trompée."
msgstr "Tuyệt, em cứ tưởng anh sẽ lại lủi mất, nhưng em mừng vì em đã nhầm."

#: game/story/home_player_corridor.dialogue
msgctxt "home_player_corridor_118d6e35"
msgid "On va commencer par le plus faible pour commencer."
msgstr "Hãy bắt đầu từ tên yếu nhất trước đã."

#: game/story/home_player_corridor.dialogue
msgctxt "home_player_corridor_f975bc7d"
msgid "{{nobu.n}} ?"
msgstr "{{nobu.n}} ư ?"

#: game/story/warehouse_exterior.dialogue
msgctxt "warehouse_exterior_57b380ac"
msgid "{{nobu.n}} ?"
msgstr "{{nobu.n}} à?"

#: game/story/home_nobu_exterior.dialogue
msgctxt "event_nobu_fight_at_home_00793742"
msgid "{{nobu.n}} ?"
msgstr "{{nobu.n}} ơi ?"

#: game/story/home_player_corridor.dialogue
msgctxt "home_player_corridor_75e68d19"
msgid "Oui, tu sais où on peut le trouver ?"
msgstr "Chuẩn rồi, anh có biết tìm nó ở đâu không ?"

#: game/story/home_player_corridor.dialogue
msgctxt "home_player_corridor_bd620019"
msgid "Aucune idée... Ce n'est pas comme si j'étais intime avec lui."
msgstr "Biết chó... Làm như anh với tụi nó thân thiết không bằng,"

#: game/story/home_player_corridor.dialogue
msgctxt "home_player_corridor_1d90a5cc"
msgid "Et tête de bite ?"
msgstr "Thằng đầu khấc thì sao ?"

#: game/story/home_player_corridor.dialogue
msgctxt "home_player_corridor_d6e21b05"
msgid "Tu veux dire {{kimochi.n}} ?[w] Il en saura peut être davantage."
msgstr "{{kimochi.n}} hở ? Có khi nó biết nhiều hơn đấy."

#: game/story/home_player_corridor.dialogue
msgctxt "home_player_corridor_8ec9247e"
msgid "Alors ne perdons pas de temps !"
msgstr "Vậy thì nhanh cái chân lên thôi !"

#: game/story/home_player_corridor.dialogue
msgctxt "home_player_corridor_19dd51da"
msgid "Au fait...[w] Cette histoire de récompense ça tient toujours ?"
msgstr "À mà này...[w] Cái vụ thưởng ấy vẫn còn không vậy ?"

#: game/story/home_player_corridor.dialogue
msgctxt "home_player_corridor_4b8cc7d0"
msgid "Bien sur !"
msgstr "Còn chứ !"

#: game/story/love_hotel_room.dialogue
msgctxt "t78c6696c22"
msgid "Bien sur !"
msgstr "Tất nhiên !"

#: game/story/home_player_corridor.dialogue
msgctxt "home_player_corridor_3a49082e"
msgid "Je me demande bien ce que tu as préparé..."
msgstr "Không biết em đã chuẩn bị gì cho anh nhỉ..."

#: game/story/home_player_corridor.dialogue
msgctxt "home_player_corridor_57c01036"
msgid "Tu... Tu verras bien idiot !"
msgstr "Rồi... Rồi anh sẽ biết thôi !"

#: game/story/home_player_corridor.dialogue
msgctxt "home_player_corridor_2fef3103"
msgid "Bordel mais qu'est ce qui vient de se passer ?!"
msgstr "Cái chuyện đéo gì vừa xảy ra vậy ?!"

#: game/story/home_player_corridor.dialogue
msgctxt "home_player_corridor_2424e376"
msgid "J'ai vraiment... fait... ça ?!"
msgstr "Mình thực sự... đã... hả ?!"

#: game/story/home_player_corridor.dialogue
msgctxt "home_player_corridor_238f2903"
msgid "Elle voulait vraiment que je me mette a genoux pour..."
msgstr "Nó thực sự muốn mình…"

#: game/story/home_player_corridor.dialogue
msgctxt "home_player_corridor_e512b865"
msgid "C'est complètement dingue."
msgstr "Ảo thật đấy."

#: game/story/home_player_corridor.dialogue
msgctxt "home_player_corridor_f9de7361"
msgid "Je ne pensais pas que mes actions modifieraient à ce point son comportement avec moi."
msgstr "Mình không nghĩ là thái độ của con bé với mình thay đổi chóng mặt tới vậy."

#: game/story/home_player_corridor.dialogue
msgctxt "home_player_corridor_f7df4fb0"
msgid "On est vraiment passés d'une extrême à l'autre."
msgstr "Đi hết từ bất ngờ này tới bất ngờ khác."

#: game/story/home_player_corridor.dialogue
msgctxt "home_player_corridor_e62d205a"
msgid "J'ai besoin de faire le vide... Il s'est passé bien trop de choses en trop peu de temps."
msgstr "Cần phải đi chạm cỏ chút mới được... Cho thông thoáng đầu óc."

#: game/story/home_player_corridor.dialogue
msgctxt "t9931b8e595"
msgid "Allez, tu peux le faire !"
msgstr "Nào, mày ơi, mày làm được mà !"

#: game/story/home_player_corridor.dialogue
msgctxt "taacebc84dc"
msgid "Tu as rêvé de ça toute ta vie !"
msgstr "Mày đã chờ khoảnh khắc này suốt cuộc đời rồi !"

#: game/story/home_player_corridor.dialogue
msgctxt "tfbb01380fe"
msgid "C'est ton moment, ne laisse personne te l'enlever !"
msgstr "Thời tới cản không kịp ! Đừng để ai cướp mất !"

#: game/story/home_player_corridor.dialogue
msgctxt "t3944054ebd"
msgid "Ne dis rien ! Ne gâche pas mon heure de gloire !"
msgstr "Suỵt suỵt ! Để bố đắm mình trong men say chiến thắng cái !"

#: game/story/home_player_corridor.dialogue
msgctxt "te8e686c2f0"
msgid "Oh non, je t'en prie, vas-y ! Va te faire latter les couilles !"
msgstr "À, xin lỗi ! Hãy tiến lên, hỡi người anh em sấm sét!"

#: game/story/home_player_corridor.dialogue
msgctxt "t0f6371900e"
msgid "Et comment, mon frère !"
msgstr "Luôn và ngay, hỡi người đồng chí cộng sản của tôi ơi ! (TL edit: feat. Monsieur Tuna, đây và nhiều dòng nữa.)"

#: game/story/home_player_corridor.dialogue
msgctxt "tcf664ac072"
msgid "{{noriko.n}} tiens-toi prête, j'arrive !"
msgstr "{{noriko.n}}, anh tới đây !"

#: game/story/home_player_corridor.dialogue
msgctxt "t6663a9251c"
msgid "[font_size=14]Oh merde, c'est l'autre tête de bite ![/font_size]"
msgstr "[font_size=14]Đcm, thằng đầu cặc lại tới hôm nay ![/font_size]"

#: game/story/home_player_corridor.dialogue
msgctxt "t701acf2248"
msgid "Je me sens sale rien que de savoir ce qui va se passer là-dedans."
msgstr "Hai quả cà run lên như xổ số kiến thiết miền Bắc khi nghĩ đến chuyện gì xảy ra trong nớ."

#: game/story/home_player_corridor.dialogue
msgctxt "t4d8fde1f4b"
msgid "[font_size=14]Je suis venu pour ma récompense ![/font_size]"
msgstr "[font_size=14]Em yêu tặng quà cho anh đi mừ?![/font_size]"

#: game/story/home_player_corridor.dialogue
msgctxt "tf971475309"
msgid "[font_size=14]Tu n'as pas le droit de te défiler ![/font_size]"
msgstr "[font_size=14]Không cho là anh hổng có đi đâu![/font_size]"

#: game/story/home_player_corridor.dialogue
msgctxt "t698697627b"
msgid "[font_size=14]Et mais c'est sympa ici...[/font_size]"
msgstr "[font_size=14]Ái chà, cái phòng đẹp quá nhỉ ![/font_size]"

#: game/story/home_player_corridor.dialogue
msgctxt "td1832a271f"
msgid "[font_size=14]Très mignon ![/font_size]"
msgstr "[font_size=14]Cute vl ![/font_size]"

#: game/story/home_player_corridor.dialogue
msgctxt "t0b8c0a7a3e"
msgid "[font_size=14]Arrête d'inspecter ma chambre et finissons-en rapidement, j'ai des choses à faire ![/font_size]"
msgstr "[font_size=14]Ngưng khám xét phòng tau cái coi. Làm cho xong đi, nhà bao việc ![/font_size]"

#: game/story/home_player_corridor.dialogue
msgctxt "tecfb510e0b"
msgid "[font_size=14]Tu vas rentrer dans le vif du sujet ? Parfait ![/font_size]"
msgstr "[font_size=14]Đánh quanh bụi rậm mãi hà ![/font_size]"

#: game/story/home_player_corridor.dialogue
msgctxt "t5261577289"
msgid "[font_size=14]Voici comment nous allons procéder, déjà j'aimerais que tu sois pieds nus et qu-[/font_size]"
msgstr "[font_size=14]Hà hà, bấy giờ thế này nhé. Em ngồi đấy, để chân trần rồ- [/font_size]"

#: game/story/home_player_corridor.dialogue
msgctxt "t988e69a3a5"
msgid "[font_size=14]Put... Pourquoi tu as frappé avec le genou ?[/font_size]"
msgstr "[font_size=14]Hự... E- Em yêu.. Sao em... Lên gối... với anh... ?[/font_size]"

#: game/story/home_player_corridor.dialogue
msgctxt "t06ea16b032"
msgid "[font_size=14]Arglll...[/font_size]"
msgstr "[font_size=14]Arghhhhh...[/font_size]"

#: game/story/home_player_corridor.dialogue
msgctxt "t7c3c1203a8"
msgid "Tu pouvais pas empêcher cet idiot d'entrer dans ma chambre ?"
msgstr "Đừng để con lợn này xông vào phòng nữa được không ?"

#: game/story/home_player_corridor.dialogue
msgctxt "tb7cad4e7d5"
msgid "C'était impossible de l'arrêter, il était déterminé à obtenir sa récompense."
msgstr "Ngăn nó làm sao được, đàn ông \"cương\" trực, rắn hơn thép, ngay hơn kiếm mà lai."

#: game/story/home_player_corridor.dialogue
msgctxt "tb51918014d"
msgid "Ce type est un crétin !"
msgstr "Mẹ cha cái thằng đầu khấc !"

#: game/story/home_player_corridor.dialogue
msgctxt "t0b03df2d2e"
msgid "Je sais..."
msgstr "Tớ cũng hiểu...1"

#: game/story/home_kimochi_exterior.dialogue
msgctxt "home_kimochi_exterior_257d9e72"
msgid "Je sais..."
msgstr "Tôi biết..."

#: game/story/home_player_livingroom.dialogue game/story/home_player_yasuka_room.dialogue
msgctxt "yasuka_love_sexe_repeat_edfa2d71"
msgid "Je sais..."
msgstr "Mẹ biết..."

#: game/story/home_player_livingroom.dialogue
msgctxt "home_player_livingroom_7df589fb"
msgid "Je sais..."
msgstr "Biết chứ ạ..."

#: game/story/home_player_hallway.dialogue
msgctxt "t000b6e0689"
msgid "Je sais..."
msgstr "Tớ cũng hiểu...2"

#: game/story/home_player_noriko_room.dialogue
msgctxt "menu_rejet_noriko_final_86653964"
msgid "Je sais..."
msgstr "Em biết..."

#: game/story/home_player_exterior.dialogue
msgctxt "asami_truth_212d13a8"
msgid "Je sais..."
msgstr "Biết chứ..."

#: game/story/home_player_corridor.dialogue
msgctxt "tf5849805d6"
msgid "Tu n'es pas avec {{asami.n}} finalement ?"
msgstr "Anh không tới cùng {{asami.n}} à ?"

#: game/story/home_player_corridor.dialogue
msgctxt "t4857709361"
msgid "Non, son père est venu la chercher sans prévenir et elle est partie passer quelques jours avec lui."
msgstr "Không, cha con họ tề tựu nhau rồi kéo nhau đi la-mát lấy mấy hôm rồi."

#: game/story/home_player_corridor.dialogue
msgctxt "tdffdab63ae"
msgid "Oh mon pauvre petit bichon se retrouve tout seul !"
msgstr "Uii, thương quá đi, bé iu của tui mất gấu rùi !"

#: game/story/home_player_corridor.dialogue
msgctxt "td62a2a62c2"
msgid "Allez, ne t'en fais pas, je m'occuperai de toi ce soir si tu veux !"
msgstr "Uây, lo gì, để em yêu đây ngủ chung giường với anh cho ấm !"

#: game/story/home_player_corridor.dialogue
msgctxt "t9c1da4a599"
msgid "Objection !"
msgstr "Phản đối !"

#: game/story/home_player_corridor.dialogue
msgctxt "t9032303c74"
msgid "Occupe-toi plutôt de moi !"
msgstr "Ngủ chung giường với anh nè !"

#: game/story/home_player_corridor.dialogue
msgctxt "t3a476f7a24"
msgid "...[w]Plutôt mourir."
msgstr "Bà thà chết còn hơn ."

#: game/story/home_player_corridor.dialogue
msgctxt "t79656c7c11"
msgid "Allez, arrête les bêtises et discutons dans ma chambre plutôt !"
msgstr "Thôi, xàm quần ở đây làm gì. Vào phòng tôi rồi chém tiếp !"

#: game/story/home_player_corridor.dialogue
msgctxt "t4f1e0d6e50"
msgid "[font_size=14]Sa suffit, dégage ![/font_size]"
msgstr "[font_size=14]Thôi, làm nữa chết mất ![/font_size]"

#: game/story/home_player_corridor.dialogue
msgctxt "tfb1c013bb8"
msgid "[font_size=14]Tu veux me tuer ou quoi ?[/font_size]"
msgstr "[font_size=14]Bộ anh muốn em quy tiên hay gì ?[/font_size]"

#: game/story/home_player_corridor.dialogue
msgctxt "t1a3c3d39db"
msgid "[font_size=14]Si on recommence une fois de plus je vais faire une crise cardiaque ![/font_size]"
msgstr "[font_size=14]Thêm nháy nữa chắc em tới nước trụy tim chết mất thôi ![/font_size]"

#: game/story/home_player_corridor.dialogue
msgctxt "tc2593d6bca"
msgid "[font_size=14]Tu n'as qu'a te finir tout seul ![/font_size]"
msgstr "[font_size=14]Về phòng mà tự thẩm ![/font_size]"

#: game/story/home_player_corridor.dialogue
msgctxt "tcdc4c5feca"
msgid "Mhh j'ai peut être un peu abusé."
msgstr "Đa dâm thì hại thận là đúng rồi."

#: game/story/home_player_corridor.dialogue
msgctxt "te633e9bd8c"
msgid "Je n'aurais pas du demander une septième fois."
msgstr "Đúng ra không nên làm tới lần thứ BẢY.(TL note: Tóc không che mặt mà khỏe hơn voi, hảo hán.)"

#: game/story/home_player_corridor.dialogue
msgctxt "t66482ca19b"
msgid "Je suppose qu'il est temps d'aller dormir."
msgstr "Chắc là nên đi ngủ thôi."

#: game/story/home_player_corridor.dialogue
msgctxt "t56ccf3cc2a"
msgid "Et maintenant c'est au tour de la mère d'{{asami.n}} ?"
msgstr "Giờ lại đến lươt mẹ của chị {{asami.n}} sao ?"

#: game/story/home_player_corridor.dialogue
msgctxt "t54aa5fe07a"
msgid "Comment ça ?"
msgstr "Chắc cũng không sao đâu nhỉ ?"

#: game/story/onsen_bath.dialogue
msgctxt "t73967b3c38"
msgid "Comment ça ?"
msgstr "Là sao ạ ?"

#: game/story/home_player_livingroom.dialogue game/story/home_player_yasuka_room.dialogue
msgctxt "talk_yasuka_4d62792e"
msgid "Comment ça ?"
msgstr "Ý mẹ là sao ạ ?"

#: game/story/konbini_interior.dialogue
msgctxt "COMMENT_A_438782"
msgid "Comment ça ?"
msgstr ""

#: game/story/bus_stop.dialogue
msgctxt "noriko_forgive_cbad80a5"
msgid "Comment ça ?"
msgstr "Ý anh... là sao... ạ ?"

#: game/story/home_asami_exterior.dialogue
msgctxt "home_asami_exterior_e68dc102"
msgid "Comment ça ?"
msgstr "Hả ?"

#: game/story/home_asami_asami_room.dialogue
msgctxt "home_asami_asami_room_fc99bce2"
msgid "Comment ça ?"
msgstr "Tại sao lại vậy hả con ?"

#: game/story/restaurant_sakuya_room.dialogue
msgctxt "restaurant_sakuya_room_9f5cdb78"
msgid "Comment ça ?"
msgstr "Ý mày là sao ?"

#: game/story/park.dialogue
msgctxt "asami_choice_9f5cdb78"
msgid "Comment ça ?"
msgstr "Vấn đề gì sao?"

#: game/story/park.dialogue
msgctxt "asami_choice_9f5cdb78_1"
msgid "Comment ça ?"
msgstr "Nghĩa là sao?"

#: game/story/home_minato_basement.dialogue
msgctxt "ta50779437b"
msgid "Comment ça ?"
msgstr "Nghĩa là sao ?"

#: game/story/sewers.dialogue
msgctxt "COMMENT_A_82FE1F"
msgid "Comment ça ?"
msgstr ""

#: game/story/home_player_corridor.dialogue
msgctxt "te34ca7e601"
msgid "Je t'en prie, tu vois très bien de quoi je veux parler."
msgstr "Anh biết em đang nhắc đến ai mà,"

#: game/story/home_player_corridor.dialogue
msgctxt "ta54c19ad04"
msgid "Depuis quelque temps tu as une fâcheuse tendance à te retrouver intime avec toutes les filles que tu rencontres."
msgstr "Dạo gần đây anh suốt ngày chỉ biết bú liếm hết bình sữa này tới bình sữa nọ."

#: game/story/home_player_corridor.dialogue
msgctxt "t9515b484fd"
msgid "...Je vais pas te contredire la dessus mais..."
msgstr "Thì cũng chẳng ai nói gì đâu nhưng mà..."

#: game/story/home_player_corridor.dialogue
msgctxt "t407d9f1bdc"
msgid "Avec {{sayuri.n}} c'est différent !"
msgstr "Với cô {{sayuri.n}} thì khác lắm nha !"

#: game/story/home_player_corridor.dialogue
msgctxt "t80a3ca40f4"
msgid "Explique moi en quoi ?"
msgstr "Khác chỗ nào ?"

#: game/story/home_player_corridor.dialogue
msgctxt "tb8fa6ebdf8"
msgid "Elle est mariée !"
msgstr "Cô ấy có chồng rồi !"

#: game/story/home_player_corridor.dialogue
msgctxt "t186315ea49"
msgid "Quoi ?! C'est tout ?"
msgstr "Ơ, có mỗi thế thôi á ?"

#: game/story/home_player_corridor.dialogue
msgctxt "t5591db1c51"
msgid "Et bien quoi ?"
msgstr "Thì... nàm thao ?"

#: game/story/home_player_corridor.dialogue
msgctxt "t40fb58d7df"
msgid "Tu penses que le fait qu'elle soit mariée la rend intouchable ?"
msgstr "Anh tưởng hoa đã có chậu thì không động được vào chắc ?"

#: game/story/home_player_corridor.dialogue
msgctxt "t4f5f8fc6c2"
msgid "Oh tu es si naïf parfois..."
msgstr "Ôi ông anh yêu quý của tôi ơi..."

#: game/story/home_player_corridor.dialogue
msgctxt "t144d043333"
msgid "Je ne suis pas assez fou pour tenter quoi que ce soit avec la mère de ma petite amie !"
msgstr "Anh không bấn tới độ húp cả mẹ vợ lẫn cô vợ nhà anh đâu !"

#: game/story/home_player_corridor.dialogue
msgctxt "t572d2fd329"
msgid "Pourtant ça t'a pas gêné de le faire avec ma mère et moi."
msgstr "Ờ, còn mẹ nhà lẫn vợ nhà thì húp ngon lành nhỉ ?"

#: game/story/home_player_corridor.dialogue
msgctxt "ta1afcea64a"
msgid "C'est... différent."
msgstr "Cái đấy... khác."

#: game/story/home_player_corridor.dialogue
msgctxt "td2c5070b86"
msgid "En quoi ?"
msgstr "Khác ở cái lồn à ?"

#: game/story/home_player_corridor.dialogue
msgctxt "tecfe73541b"
msgid "Je plains l'autre tête de bite..."
msgstr "Em thấy buồn cho cái anh Đầu Khấc quớ..."

#: game/story/home_player_corridor.dialogue
msgctxt "t1bbf64f9ef"
msgid "Qu'est ce que {{kimochi.n}} à avoir là dedans ?"
msgstr "Tự dưng lôi {{kimochi.n}} vô đây làm gì ?"

#: game/story/home_player_corridor.dialogue
msgctxt "t87b3fc6ea1"
msgid "Il va encore te voir avec une nouvelle femme à tes côtés alors que lui va probablement finir sa vie puceau."
msgstr "Ảnh sẽ mãi kiếp trai tân, rồi tới mức chết thối trong cái xó ẩm thấp của ảnh, trong khi thằng bạn chí cốt ngay kề thì đi rúc chim vào hết cô này tới bà khác."

#: game/story/home_player_corridor.dialogue
msgctxt "t62830abfad"
msgid "Il ne se passera rien avec elle !"
msgstr "Thề, anh không bấn tới mức đấy !"

#: game/story/home_player_corridor.dialogue
msgctxt "t5b02d0740d"
msgid "Tu sous-estimes trop le sex-appeal d'une femme mariée mon pauvre {{saito.n}}"
msgstr "Anh {{saito.n}} ơi là zời, đã là dân sành ăn thì không cưỡng lại được đặc sản đâu !"

#: game/story/home_player_corridor.dialogue
msgctxt "t34550aa936"
msgid "...[w]N'importe quoi..."
msgstr "...[w]Sao cũng được..."

#: game/story/home_player_corridor.dialogue
msgctxt "te66c6edf85"
msgid "[font_size=14]Et rappelle toi...[/font_size]"
msgstr "[font_size=14]Và nhớ lấy một điều...[/font_size]"

#: game/story/home_player_corridor.dialogue
msgctxt "tb1ec01f44d"
msgid "[font_size=14]On ne touche pas pas à la mère de sa meilleure amie[/font_size]"
msgstr "[font_size=14]Đừng có làm gì mấy bà mẹ đấy nhé![/font_size]"

#: game/story/home_player_corridor.dialogue
msgctxt "t4b748d8a3a"
msgid "[font_size=14]On ne touche pas pas à la mère de sa petite amie[/font_size]"
msgstr "[font_size=14]Đừng có làm gì mấy bà mẹ đấy nhé![/font_size]"

#: game/story/home_player_corridor.dialogue
msgctxt "tc4bea41113"
msgid "Je t'ai déjà dis qu'il ne se passerait rien !"
msgstr "Đã bảo là sẽ không có gì rồi mà !"

#: game/story/home_player_corridor.dialogue
msgctxt "te8244f15f1"
msgid "[font_size=14]C'est ce qu'ils disent tous...[/font_size]"
msgstr "[font_size=14]Moẹ cái ngữ của đám sành ăn...[/font_size]"

#: game/story/home_player_corridor.dialogue
msgctxt "teb79a71097"
msgid "Oh merde !"
msgstr "Đuỵt !"

#: game/story/home_player_corridor.dialogue
msgctxt "t583d7b4288"
msgid "Tu m'as fait peur, je ne m'attendais pas à te trouver là."
msgstr "Sợ vãi tè, ẩn núp ở đây làm chi ?"

#: game/story/home_player_corridor.dialogue
msgctxt "t00543a8508"
msgid "Tu rentres tard, une fois de plus."
msgstr "Lại đi chơi tới quên lối về ha."

#: game/story/home_player_corridor.dialogue
msgctxt "te78d30b416"
msgid "Oui je sais..."
msgstr "Yeah, muộn thật..."

#: game/story/home_player_corridor.dialogue
msgctxt "t4beca839c3"
msgid "On devait passer la journée en ville et, au final, on est restés à la salle d'arcade."
msgstr "Đúng ra tụi này định đi dạo phố, rồi thế nào lại rúc đầu vô Tiệm Arcade."

#: game/story/home_player_corridor.dialogue
msgctxt "t95d51d26b9"
msgid "Donc c'est terminé ?"
msgstr "Rồi hết... đúng không ?"

#: game/story/home_player_corridor.dialogue
msgctxt "tad373f642e"
msgid "mhh pas exactement..."
msgstr "Ờm... cũng không hẳn"

#: game/story/home_player_corridor.dialogue
msgctxt "tda848956db"
msgid "Tu la vois toujours demain, c'est ça ?"
msgstr "Ngủ dậy mai lại quấn lấy nhau tiếp, hở ?"

#: game/story/home_player_corridor.dialogue
msgctxt "t74d7ccd50e"
msgid "On va au parc d'attractions..."
