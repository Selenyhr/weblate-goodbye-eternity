msgstr ""

#: game/story/bus_stop.dialogue
msgctxt "CALME_TOI_CE_N_EST_PAS_MON_PRE_9F842E"
msgid "Calme-toi, ce n'est pas mon premier rodéo."
msgstr ""

#: game/story/bus_stop.dialogue
msgctxt "J_AI_PASS_LA_PLUPART_DE_MA_VIE_7C005D"
msgid "J'ai passé la plupart de ma vie à les esquiver, et je m'en sors plutôt bien."
msgstr ""

#: game/story/bus_stop.dialogue
msgctxt "LAISSE_MOI_R_FL_CHIR_JUSTE_QUE_2E7F03"
msgid "Laisse-moi réfléchir juste quelques secondes et..."
msgstr ""

#: game/story/bus_stop.dialogue
msgctxt "FERME_LA_ON_VA_CHEZ_MOI_C5403B"
msgid "Ferme-la, on va chez moi !"
msgstr ""

#: game/story/bus_stop.dialogue
msgctxt "EUH_OKAY_B5DDE2"
msgid "Euh... okay."
msgstr ""

#: game/story/bus_stop.dialogue
msgctxt "ON_N_A_QU_UTILISER_L_AGENT_DE_DBD5FC"
msgid "On n’a qu’à utiliser l'agent de police devant la maison."
msgstr ""

#: game/story/bus_stop.dialogue
msgctxt "C_EST_MERDE_C_EST_UNE_SUPER_ID_C6B35C"
msgid "C'est... Merde, c'est une super idée, pourquoi j'y ai pas pensé plus tôt ?"
msgstr ""

#: game/story/bus_stop.dialogue
msgctxt "ILS_NE_POURRONT_PAS_LAISSER_DE_0A4D57"
msgid "Ils ne pourront pas laisser des citoyens se faire agresser sous leurs yeux sans rien faire."
msgstr ""

#: game/story/bus_stop.dialogue
msgctxt "HEY_ELLE_EST_D_J_PARTIE_A38323"
msgid "Hey ! Elle est déjà partie..."
msgstr ""

#: game/story/bus_stop.dialogue
msgctxt "ALORS_CET_ENTRA_NEMENT_5CEA47"
msgid "Alors cet entraînement ?"
msgstr ""

#: game/story/bus_stop.dialogue
msgctxt "COMMENT_TU_FAIS_POUR_TOUJOURS_3B30A8"
msgid "Comment tu fais pour toujours savoir où je suis ?"
msgstr ""

#: game/story/bus_stop.dialogue
msgctxt "INTUITION_F_MININE_JE_SUPPOSE_48AEA2"
msgid "Intuition féminine je suppose."
msgstr ""

#: game/story/bus_stop.dialogue
msgctxt "C_TAIT_TR_S_PRODUCTIF_E28F65"
msgid "C'était... très productif !"
msgstr ""

#: game/story/bus_stop.dialogue
msgctxt "TU_N_AS_TOUJOURS_PAS_D_ENDROIT_5329B6"
msgid "Tu n’as toujours pas d’endroit où dormir ?"
msgstr ""

#: game/story/bus_stop.dialogue
msgctxt "JE_N_AI_TOUJOURS_PAS_D_ENDROIT_C0ACA4"
msgid "Je n’ai toujours pas d’endroit où dormir en effet..."
msgstr ""

#: game/story/bus_stop.dialogue
msgctxt "OKAY_C_EST_D_CID_C4A100"
msgid "Okay, c’est décidé !"
msgstr ""

#: game/story/bus_stop.dialogue
msgctxt "JE_T_ACCUEILLE_OFFICIELLEMENT_E59222"
msgid "Je t’accueille officiellement chez moi le temps nécessaire."
msgstr ""

#: game/story/bus_stop.dialogue
msgctxt "C_EST_VRAI_MAIS_C7DD75"
msgid "C’est vrai ?! Mais..."
msgstr ""

#: game/story/bus_stop.dialogue
msgctxt "NE_POSE_PAS_DE_QUESTIONS_LA_SI_D1970E"
msgid "Ne pose pas de questions, la situation est déjà assez compliquée comme ça..."
msgstr ""

#: game/story/bus_stop.dialogue
msgctxt "REMERCIE_MOI_EN_PRENANT_UNE_DO_A1A26C"
msgid "Remercie-moi en prenant une douche plutôt !"
msgstr ""

#: game/story/bus_stop.dialogue
msgctxt "LA_NUIT_EST_D_J_TOMB_E_FAF6A3"
msgid "La nuit est déjà tombée..."
msgstr ""

#: game/story/bus_stop.dialogue
msgctxt "FORCE_DE_TRA_NER_DANS_CES_GOU_EB2E3D"
msgid "À force de traîner dans ces égouts, je perds complètement la notion du temps."
msgstr ""

#: game/story/bus_stop.dialogue
msgctxt "PEUT_TRE_QU_ELLE_M_A_ATTENDU_P_862337"
msgid "Peut-être qu'elle m’a attendu... puis qu’elle a fini par rentrer."
msgstr ""

#: game/story/bus_stop.dialogue
msgctxt "W_POURQUOI_EST_CE_QUE_J_ESP_R_D3B8E7"
msgid "...[w] Pourquoi est-ce que j'espérais qu'elle serait là ?"
msgstr ""

#: game/story/bus_stop.dialogue
msgctxt "ELLE_BOSSE_AVEC_MINATO_N_POURQ_E534B2"
msgid "Elle bosse avec {{minato.n}}, pourquoi j'oublie ça a chaque fois ?!"
msgstr ""

#: game/story/bus_stop.dialogue
msgctxt "QUELLE_JOURN_E_71BB29"
msgid "Quelle journée !!"
msgstr ""

#: game/story/bus_stop.dialogue
msgctxt "JE_SENS_QUE_JE_SUIS_PR_T_F65B37"
msgid "Je sens que je suis prêt."
msgstr ""

#: game/story/bus_stop.dialogue
msgctxt "C_EST_FOU_LES_PROGR_S_QUE_J_AI_862F5E"
msgid "C’est fou les progrès que j’ai pu faire en un laps de temps aussi court."
msgstr ""

#: game/story/bus_stop.dialogue
msgctxt "EST_CE_QU_ON_PEUT_VRAIMENT_ENC_A6D97F"
msgid "Est-ce qu’on peut vraiment encore parler de progrès à ce niveau-là... ?"
msgstr ""

#: game/story/bus_stop.dialogue
msgctxt "J_AI_ENFIN_LA_FORCE_N_CESSAIRE_96104B"
msgid "J'ai enfin la force nécessaire pour protéger tout le monde."
msgstr ""

#: game/story/bus_stop.dialogue
msgctxt "ATTENDS_UN_PEU_MINATO_N_ET_ON_A80424"
msgid "Attends un peu {{minato.n}} et on verra si te cacher derrière {{netoru.n}} sera suffisant !"
msgstr ""

#: game/story/bus_stop.dialogue
msgctxt "ALLEZ_NATSUME_N_DOIT_PROBABLEM_69A207"
msgid "Allez... {{natsume.n}} doit probablement déjà m’attendre !"
msgstr ""

#: game/story/lake.dialogue
msgctxt "lake_31ede489"
msgid "Viens t'asseoir, on a pas mal de temps à tuer en attendant ce soir !"
msgstr "Tới ngồi cạnh bên nè anh, còn phải chờ tới dài cổ ra đó !"

#: game/story/lake.dialogue
msgctxt "lake_0af37d08"
msgid "A quoi tu penses ?"
msgstr "Anh thấy thế nào ?"

#: game/story/lake.dialogue
msgctxt "lake_38bc82eb"
msgid "Je me disais que j'avais vraiment du mal a te suivre..."
msgstr "...Bấy lâu nay, anh đã luôn nghĩ... rằng thật khó để hiểu được... em..."

#: game/story/lake.dialogue
msgctxt "lake_25dc4344"
msgid "On a d'abord eu une relation très forte... Ça a changé pour diverses raisons."
msgstr "Xưa kia tụi mình thân thiết biết bao... Thế rồi vì lý do nào đó mà mọi sự đổ bể."

#: game/story/lake.dialogue
msgctxt "lake_5781bcdf"
msgid "Tu m'as fait vivre un enfer, genre littéralement."
msgstr "Em như quỷ thần dùng nhục hình phanh thây anh, thật luôn đấy."

#: game/story/lake.dialogue
msgctxt "lake_85feaee9"
msgid "Tout ça pour mon bien..."
msgstr "Tất cả vì anh hết..."

#: game/story/lake.dialogue
msgctxt "lake_b0f0928f"
msgid "... et aujourd'hui tu es là m'aider à changer pour le mieux."
msgstr "... và giờ em ở đây hôm nay để chắp anh đôi cánh, đưa anh khỏi nơi hỏa ngục rực lửa."

#: game/story/lake.dialogue
msgctxt "lake_046c9aaf"
msgid "Et je ne mentionne pas ce qu'on a fait toi et moi hier dans la chambre..."
msgstr "Còn cả những gì xảy ra hôm qua trong phòng em nữa..."

#: game/story/lake.dialogue
msgctxt "lake_ae148ef7"
msgid "Et je ne mentionne pas la proposition que tu m'as faite hier dans la chambre..."
msgstr "Còn cả mấy lời em nói hôm qua trong phòng em nữa..."

#: game/story/lake.dialogue
msgctxt "lake_559dc029"
msgid "Nous ne sommes pas frère et sœur donc je ne vois pas où est le problème ?"
msgstr "Em và anh có máu mủ gì đâu, nên em thấy hổng có sao hết á !"

#: game/story/lake.dialogue
msgctxt "lake_02dec0f4"
msgid "...Donc on oublie tout le passé et on se concentre sur l'avenir en gros."
msgstr "...Vậy thì, đã đến lúc buông bỏ quá khứ và hướng tới tương lai rồi, em nhỉ ?"

#: game/story/lake.dialogue
msgctxt "lake_ff89d187"
msgid "C'est à peu près ça..."
msgstr "Đâu ai đeo hận cả đời được..."

#: game/story/lake.dialogue
msgctxt "lake_4c9dbb2c"
msgid "Tu prévois une autre récompense aussi si je mets une branlée à {{kenji.n}} ?"
msgstr "Anh có được thưởng thêm cua móc sau khi lóc xương thằng {{kenji.n}} không ?"

#: game/story/lake.dialogue
msgctxt "lake_4d32bf5d"
msgid "Je demandais juste..."
msgstr "Anh chỉ hỏi thôi mà..."

#: game/story/lake.dialogue
msgctxt "lake_d9096b73"
msgid "Tu aimerais ?"
msgstr "Anh... có thích không ?"

#: game/story/lake.dialogue
msgctxt "lake_63fe5d60"
msgid "Oh monsieur {{saito.n}} préfère camper sur sa position ?"
msgstr "hihi, anh {{saito.n}} yêu quý giờ còn biết cả lịch thiệp cơ đấy !"

#: game/story/lake.dialogue
msgctxt "lake_2536c814"
msgid "Comme tu voudras... Mais c'est toi qui rate quelque chose."
msgstr "Tùy anh thôi... Nhưng trẻ không chơi là đang đánh rơi tuổi trẻ đó nha."

#: game/story/lake.dialogue
msgctxt "t7932c4e3c3"
msgid "Ne dis pas de bêtises !"
msgstr "Bớt xàm xí lại nàng ạ !"

#: game/story/lake.dialogue
msgctxt "lake_ab9924e2"
msgid "Je vais prendre ton silence pour un oui."
msgstr "Im lặng là đồng ý nhé."

#: game/story/lake.dialogue
msgctxt "lake_93d7880d"
msgid "Comme quoi, tu apprends vite de tes erreurs..."
msgstr "Anh có vẻ học hỏi nhanh đấy nhỉ..."

#: game/story/lake.dialogue
msgctxt "lake_e514c953"
msgid "Regardez moi ce pervers qui en veut toujours plus !"
msgstr "Gớm chửa, cái mồm điêu, chỉ có văn là giỏi ! (Trans note: Còn mình thì toàn lẹt đet có 6 huhu)"

#: game/story/lake.dialogue
msgctxt "lake_10e113ae"
msgid "La dernière fois ne t'a pas suffit ?"
msgstr "Lần trước là chưa đủ thỏa mãn anh sao ?"

#: game/story/lake.dialogue
msgctxt "t42df33eaac"
msgid "Moi je prends tout l'amour qu'on est prêt à me donner."
msgstr "Anh nhận lấy mọi tình yêu được ban tặng."

#: game/story/lake.dialogue
msgctxt "lake_f86bcf37"
msgid "Je crois qu'il est temps d'aller s'occuper de {{kenji.n}}."
msgstr "Chắc là tới lúc mình đi róc xương thằng {{kenji.n}} đem ninh nước phở rồi em."

#: game/story/lake.dialogue
msgctxt "lake_bdf9ac4a"
msgid "Oui!"
msgstr "Dạạạ ! (Trans note: Tiếng dạ như thắp lên ngọn lửa le lót trong trái chym đã nguội lạnh và khô cứng như gỗ đá của tui. Hiu hiu ngày hôm nay ăn phải bốn suất cơm chó rồi !!!)"

#: game/story/lake.dialogue
msgctxt "lake_34cb23a5"
msgid "Et bien alors ?"
msgstr "Chà chà ?"

#: game/story/park.dialogue
msgctxt "t91222cb41e"
msgid "Et bien alors ?"
msgstr "Có chuyện gì hở anh ?"

#: game/story/lake.dialogue
msgctxt "lake_f301c440"
msgid "Maintenant qu'on en a terminé tu m'ignores de nouveau ?"
msgstr "bây giờ nhà hêt việc rồi, em lại tính bơ anh nữa sao ?"

#: game/story/lake.dialogue
msgctxt "lake_8ca1fc62"
msgid "{{noriko.n}}... Tu m'as aidé pour me reprendre en main... À ta façon bien sûr."
msgstr "{{noriko.n}}à... Em đã giúp tôi rèn nên một phần con người anh... theo những cách rất riêng, đặc biệt, của chính em."

#: game/story/lake.dialogue
msgctxt "lake_a98e3e4f"
msgid "Mais je vois bien qu'il y a toujours quelque chose qui te préoccupe."
msgstr "Nhưng cái anh đang thấy là, có vẻ như em vẫn đang còn chút vướng bận gì đó trong lòng."

#: game/story/lake.dialogue
msgctxt "lake_47ca85e9"
msgid "Quand j'y repense... tu semblais terriblement pressée que j'en finisse avec eux."
msgstr "Ngẫm lại, anh thấy... cảm giác như em luôn rất gấp gáp về chuyện đốc suất anh cho ba tên Lính Siêu Cấp kia ăn bả."

#: game/story/lake.dialogue
msgctxt "lake_07b1d2c1"
msgid "Comme s'il y avait une date limite à tout ça..."
msgstr "Cứ như là... Bị deadline dí ấy, em ạ..."

#: game/story/lake.dialogue
msgctxt "lake_021b5183"
msgid "Laisse-moi t'aider pour une fois."
msgstr "Hãy để anh giúp em một lần, nhé ?"

#: game/story/lake.dialogue
msgctxt "lake_9fb4f4f9"
msgid "Laisse tomber, il est trop tard à présent."
msgstr "Thôi bỏ đi anh, đã quá trễ rồi."

#: game/story/lake.dialogue
msgctxt "lake_abe858dc"
msgid "S'il y a bien un truc que j'ai appris ces derniers temps c'est justement qu'il n'est jamais trop tard."
msgstr "Sống trên đời, em ạ, nếu như có bài học gì mà anh đã nhét được vào cái não cá vàng của anh, thì đó là: Chẳng bao giờ quá muộn cho một khởi đầu mới."

#: game/story/lake.dialogue
msgctxt "lake_15f75d5d"
msgid "...[w] Tu ne pourras rien contre lui, personne ne peut."
msgstr "...[w] Anh không làm gì được hắn ta đâu. Chẳng ai có đủ trình."

#: game/story/lake.dialogue
msgctxt "lake_f60eb0c3"
msgid "Ce n'est pas à toi d'en décider, maintenant dis-moi de qui tu parles."
msgstr "Đằng nào thì hội chị em các em chả đứng ngoài cổ động ? Nào, giờ, nói nhanh ! Ai ?"

#: game/story/lake.dialogue
msgctxt "lake_1db034d5"
msgid "Pfff comme tu voudras..."
msgstr "Pfff, dạ vâng, thưa anh..."

#: game/story/lake.dialogue
msgctxt "lake_b78879cb"
msgid "Malheureusement pour moi à l'école, le fait qu'elle m'ait élevée seule a fait de moi un cas à part."
msgstr "Thưa chuyện với anh từ đầu nè. Ngày xửa ngày xưa, một đứa vô cha, là em đây, luôn bị liệt vào cái trường hợp 'cá biệt'ở trường lớp."

#: game/story/lake.dialogue
msgctxt "lake_13990668"
msgid "À leurs yeux je n'étais que la fille d'une pute..."
msgstr "Từ bạn bè, tới thầy cô, trong mắt chúng nó, ở cái trường thối rách đó, em chỉ là một đứa con hoang của một con đĩ lăng loàn..."

#: game/story/lake.dialogue
msgctxt "lake_d982de1e"
msgid "Mais... tu étais la seule personne à me montrer de la gentillesse."
msgstr "Trong cái kỷ tăm tối đó... Chỉ có anh là người duy nhất đem lại ánh sáng cho em."

#: game/story/lake.dialogue
msgctxt "lake_3b6a7f58"
msgid "Après j'ai eu cette idée débile de faire appel aux trois idiots..."
msgstr "thế rồi, ta tua đến cái lúc mà em có cái ý tưởng chết não, đi nhờ ba con Lĩnh Siêu Cấp ấy..."

#: game/story/lake.dialogue
msgctxt "lake_181ed5e2"
msgid "Ah ça..."
msgstr "À rồi, ok..."

#: game/story/lake.dialogue
msgctxt "lake_770b9b12"
msgid "Mais... À présent qu'on en a terminé avec les thugs..."
msgstr "Thế giờ... Sau khi đã đắp mộ cuộc tình mấy tên kia rồi này..."

#: game/story/lake.dialogue
msgctxt "lake_a4f9f20e"
msgid "Il va falloir s'occuper de tous ceux qui nous ont causé du tort."
msgstr "Em muốn đi xử lý, nhanh nhất có thể, bất cứ những kẻ nào đã gây hại cho gia đình ta."

#: game/story/lake.dialogue
msgctxt "lake_03e2b765"
msgid "Ils sont trop nombreux et je voulais en finir vite."
msgstr "Cái danh sách nó phải dài lê thê ý, vậy nên em mới đi nhờ một tay, đi tử hình tập thể cùng cho tiện."

#: game/story/lake.dialogue
msgctxt "lake_91bb1bb2"
msgid "C'est pour ça que j'ai demandé l'aide d'{{akira.n}}."
msgstr "Và thế là, duyên nợ thế nào đẩy em tới chỗ {{akira.n}}."

#: game/story/lake.dialogue
msgctxt "lake_f2adc391"
msgid "Il avait une sale réputation qui n'était plus à faire... Il me protégerait en échange de..."
msgstr "Tên chó má tay đã nhúng chàm không biết bao nhiêu lần... Hắn hứa sẽ bảo vệ nhà ta, đổi lại thì..."

#: game/story/lake.dialogue
msgctxt "lake_4e3da6c0"
msgid "Je n'aime pas la direction que prend cette conversation..."
msgstr "... Anh cũng hiểu... hòm hòm rồi đây..."

#: game/story/lake.dialogue
msgctxt "lake_1329b19a"
msgid "Je devais devenir son plan cul personnel."
msgstr "Em phải trở thành... búp bê tình dục của hắn."

#: game/story/lake.dialogue
msgctxt "lake_a4c11ba3"
msgid "Putain j'en étais sur, ce type est une véritable ordure."
msgstr "Anh biết ngay mà ! Thằng chó khốn kiếp ấy !"

#: game/story/lake.dialogue
msgctxt "lake_204d0eb2"
msgid "J'ai refusé mais il m'a menacée de s'en prendre à maman..."
msgstr "Em đã cố từ chối, nhưng hắn dọa rằng sẽ tìm tới mẹ..."

#: game/story/lake.dialogue
msgctxt "lake_e8d5dd26"
msgid "{{saito.n}} j'ai vraiment peur, ce type est capable de tout..."
msgstr "{{saito.n}}, anh ơi... Em... sợ lắm... Tên đấy giết người chẳng ghê tay..."

#: game/story/lake.dialogue
msgctxt "lake_cad9bc1f"
msgid "Ne t'en fais pas, j'en fais mon affaire !"
msgstr "Đừng lo em à, có gì khó, có anh lo !"

#: game/story/lake.dialogue
msgctxt "lake_ee2203c6"
msgid "Mais il est bien plus fort que toi !"
msgstr "Nhưng, thằng chó nớ, chẳng lìu tìu như đám rẻ rách kia đâu !"

#: game/story/lake.dialogue
msgctxt "lake_cbacebf4"
msgid "Plus maintenant !"
msgstr "Vậy hôm nay, anh sẽ cho nó nếm mùi 'lìu tìu tép riu' !"

#: game/story/lake.dialogue
msgctxt "lake_9a208089"
msgid "Laisse moi m'occuper de ça !"
msgstr "Chỉ cần cho nếm một 'chưởng' của anh là xong xuôi hết !"

#: game/story/lake.dialogue
msgctxt "lake_dbf6a15d"
msgid "Je te promets qu'il n'essaiera plus de s'en prendre à toi, tant que je serai là."
msgstr "Anh hứa với em, rằng nó chẳng thể rờ đến một sợi lông chân của bất cứ ai trong nhà ta."

#: game/story/lake.dialogue
msgctxt "lake_1aad3c91"
msgid "Tu devais le retrouver où ?"
msgstr "Em phải gặp nó ở đâu nào !"

#: game/story/lake.dialogue
msgctxt "lake_38d2681e"
msgid "Ce soir, devant le love hotel."
msgstr "Tối nay, trước cửa Tình Phủ."

#: game/story/lake.dialogue
msgctxt "lake_e2a87085"
msgid "Très bien, rentre a la maison."
msgstr "Được. Trước tiên thì, hãy về nhà nghỉ đi đã, em nhé. Em đã phải lo nghĩ nhiều rồi."

#: game/story/lake.dialogue
msgctxt "lake_667b9231"
msgid "Je vais gérer ça personnellement !"
msgstr "Hãy đánh một giấc thật ngon, và chờ tới khi anh về loan báo tin mừng nhé !"

#: game/story/lake.dialogue
msgctxt "lake_72454bac"
msgid "Fais... Fais attention à toi !"
msgstr "Hãy... Giữ mình nhé anh !"

#: game/story/lake.dialogue
msgctxt "lake_a0248723"
msgid "...[w] Je pourrais apprécier ce côté où tu t'inquiètes pour moi tu sais..."
msgstr "....[w]Nghe nàng đấy xong cái, anh thấy ắm lòng hẳn..."

#: game/story/lake.dialogue
msgctxt "lake_5386ef61"
msgid "Idiot ! Je m'inquiète toujours pour toi !"
msgstr "Cái anh này ! Lúc nào mà... em chẳng lo cho anh !"

#: game/story/lake.dialogue
msgctxt "PFFF_J_EN_AI_MARRE_AD633A"
msgid "Pfff... J'en ai marre."
msgstr ""

#: game/story/lake.dialogue
msgctxt "COMMENT_C_EST_POSSIBLE_QUE_TOU_2BF9CA"
msgid "Comment c'est possible que tout ait dégénéré à ce point ?!"
msgstr ""

#: game/story/lake.dialogue
