msgstr ""

#: game/story/home_natsume_natsume_room.dialogue
msgctxt "UNE_CONDITION_QUE_JE_BOSSE_PO_61230A"
msgid "À une condition : que je bosse pour lui, que je fasse ses sales boulots."
msgstr ""

#: game/story/home_natsume_natsume_room.dialogue
msgctxt "SI_JE_D_SOB_ISSAIS_UN_SIMPLE_C_0BFA32"
msgid "Si je désobéissais, un simple coup de fil et c’était fini pour elle."
msgstr ""

#: game/story/home_natsume_natsume_room.dialogue
msgctxt "AUJOURD_HUI_J_AI_ENCORE_LE_DRO_DD2ED4"
msgid "Aujourd’hui, j’ai encore le droit de la voir de temps en temps. On parle un peu."
msgstr ""

#: game/story/home_natsume_natsume_room.dialogue
msgctxt "ELLE_S_ACCROCHE_MAIS_JE_VOIS_B_BAA004"
msgid "Elle s’accroche, mais... je vois bien qu’elle a déjà baissé les bras."
msgstr ""

#: game/story/home_natsume_natsume_room.dialogue
msgctxt "EN_ATTENDANT_JE_METS_DE_C_T_PO_9F3E19"
msgid "En attendant, je mets de côté pour un vrai avocat. Quelqu’un de bien."
