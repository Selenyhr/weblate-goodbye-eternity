#: game/story/home_natsume_natsume_room.dialogue
msgctxt "C_EST_D_ACCORD_C42DD1"
msgid "C'est d'accord !"
msgstr ""

#: game/story/home_natsume_natsume_room.dialogue
msgctxt "JE_TE_LE_PROMETS_DE689D"
msgid "Je te le promets..."
msgstr ""

