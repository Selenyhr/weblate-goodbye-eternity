#: game/story/home_natsume_natsume_room.dialogue
msgctxt "_869C38"
msgid ".........."
msgstr ""

#: game/story/home_natsume_natsume_room.dialogue
msgctxt "LA_R_INCARNATION_A_A_L_AIR_BIE_E15794"
msgid "La réincarnation... Ça a l’air bien, dit comme ça."
msgstr ""

#: game/story/home_natsume_natsume_room.dialogue
msgctxt "SI_C_TAIT_R_EL_JE_REMONTERAIS_6162C9"
msgid "Si c’était réel, je remonterais le temps... et je le tuerais moi-même."
msgstr ""

#: game/story/home_natsume_natsume_room.dialogue
msgctxt "W_TU_SAIS_QUOI_T_AVAIS_RAISON_8C048B"
msgid "...[w] Tu sais quoi ? T’avais raison."
msgstr ""

#: game/story/home_natsume_natsume_room.dialogue
msgctxt "EN_PARLER_A_M_A_SOULAG_E_JE_ME_7CC7D2"
msgid "En parler, ça m’a soulagée. Je me sens plus légère."
msgstr ""

#: game/story/home_natsume_natsume_room.dialogue
msgctxt "OH_JE_SENS_QUE_MAINTENANT_C_ES_AD9AC1"
msgid "Oh, je sens que maintenant, c’est toi qui meurs d’envie de parler."
msgstr ""

#: game/story/home_natsume_natsume_room.dialogue
msgctxt "QUEL_DOMMAGE_9F585F"
msgid "Quel dommage..."
msgstr ""

#: game/story/home_natsume_natsume_room.dialogue
msgctxt "RHAAAA_T_ES_S_RIEUSE_DAABF0"
msgid "Rhaaaa, t’es sérieuse ?!"
msgstr ""

#: game/story/home_natsume_natsume_room.dialogue
msgctxt "TU_AS_PROMIS_BONNE_NUIT_FAIS_D_BB3D7D"
msgid "Tu as promis ! Bonne nuit, fais de beaux rêves..."
msgstr ""

#: game/story/home_natsume_natsume_room.dialogue
msgctxt "JE_TE_D_TESTE_B895C8"
msgid "Je te déteste."
msgstr ""

#: game/story/home_natsume_natsume_room.dialogue
msgctxt "VRAIMENT_MOI_JE_T_AIME_BIEN_PO_9A471F"
msgid "Vraiment ? Moi je t’aime bien pourtant."
msgstr ""

#: game/story/home_natsume_natsume_room.dialogue
msgctxt "W_TU_ES_D_MONIAQUE_2AB05C"
msgid "...[w] Tu es démoniaque."
msgstr ""

#: game/story/home_natsume_natsume_room.dialogue
msgctxt "DEBOUT_LA_MARMOTTE_33CDF0"
msgid "Debout, la marmotte !"
msgstr ""

#: game/story/home_natsume_natsume_room.dialogue
msgctxt "QU_338E3D"
msgid "Qu... ?!"
msgstr ""

#: game/story/home_natsume_natsume_room.dialogue
msgctxt "J_AI_MERVEILLEUSEMENT_BIEN_DOR_A70F86"
msgid "J'ai merveilleusement bien dormi !"
msgstr ""

#: game/story/home_natsume_natsume_room.dialogue
msgctxt "T_AVAIS_RAISON_PARLER_M_A_FAIT_E24B2F"
msgid "T'avais raison, parler m'a fait énormément de bien."
msgstr ""

#: game/story/home_natsume_natsume_room.dialogue
msgctxt "J_AI_TOUJOURS_LA_PRESSION_MAIS_1D8D02"
msgid "J'ai toujours la pression, mais elle est moins lourde."
msgstr ""

#: game/story/home_natsume_natsume_room.dialogue
msgctxt "TOI_PAR_CONTRE_T_AS_UNE_SALE_T_A57090"
msgid "Toi, par contre... t’as une sale tête !"
msgstr ""

#: game/story/home_natsume_natsume_room.dialogue
msgctxt "QUI_LA_FAUTE_SELON_TOI_67222C"
