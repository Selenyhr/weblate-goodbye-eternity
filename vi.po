msgstr ""

#: game/story/home_player_noriko_room.dialogue
msgctxt "RHAAAAA_786D8E"
msgid "Rhaaaaa !"
msgstr ""

#: game/story/home_player_noriko_room.dialogue
msgctxt "JE_VAIS_DEVENIR_FOLLE_ABCA98"
msgid "Je vais devenir folle..."
msgstr ""

#: game/story/home_player_noriko_room.dialogue
msgctxt "MAMAN_B7A376"
msgid "Maman...!"
msgstr ""

#: game/story/home_player_noriko_room.dialogue
msgctxt "JE_DEVIENS_FOLLE_JE_NE_PARVIEN_B9E7A7"
msgid "Je deviens folle, je ne parviens plus à rester calme !"
msgstr ""

#: game/story/home_player_noriko_room.dialogue
msgctxt "COMMENT_FAIS_TU_POUR_NE_PAS_EX_5E58CC"
msgid "Comment fais-tu pour ne pas exploser ?"
msgstr ""

#: game/story/home_player_noriko_room.dialogue
msgctxt "J_AI_JUSTE_ENVIE_D_HURLER_849585"
msgid "J'ai juste envie d'hurler..."
msgstr ""

#: game/story/home_player_noriko_room.dialogue
msgctxt "MAMAN_W_TOUT_VA_BIEN_9E14F0"
msgid "Maman ?[w] Tout va bien ?"
msgstr ""

#: game/story/home_player_noriko_room.dialogue
msgctxt "JE_SUIS_D_SOL_E_MA_CH_RIE_JE_S_08CE73"
msgid "Je suis désolée ma chérie, je suis un peu préoccupée en ce moment."
msgstr ""

#: game/story/home_player_noriko_room.dialogue
msgctxt "TU_PARLES_DE_SAITO_N_ED978A"
msgid "Tu parles de {{saito.n}} ?"
msgstr ""

#: game/story/home_player_noriko_room.dialogue
msgctxt "PAS_SEULEMENT_SAYURI_N_LA_M_RE_35E8EA"
msgid "Pas seulement... {{sayuri.n}}, la mère d'{{asami.n}}, est coincée chez elle depuis deux jours. Ils ont également un agent de police qui la surveille."
msgstr ""

#: game/story/home_player_noriko_room.dialogue
msgctxt "COMME_NOUS_E65F81"
msgid "Comme nous..."
msgstr ""

#: game/story/home_player_noriko_room.dialogue
msgctxt "NON_C_EST_DIFF_RENT_ILS_M_EMP_D95AC9"
msgid "Non, c'est différent, ils m'empêchent d'aller la voir."
msgstr ""

#: game/story/home_player_noriko_room.dialogue
msgctxt "JE_L_AI_ENTENDUE_CRIER_ET_JE_N_D37873"
msgid "Je l'ai entendue crier et je ne crois pas à leurs explications."
msgstr ""

#: game/story/home_player_noriko_room.dialogue
msgctxt "ILS_SONT_S_REMENT_EN_TRAIN_DE_E0B82F"
msgid "Ils sont sûrement en train de lui faire quelque chose et il m'est impossible de lui venir en aide."
msgstr ""

#: game/story/home_player_noriko_room.dialogue
msgctxt "ILS_CACHENT_D_FINITIVEMENT_QUE_E4962B"
msgid "Ils cachent définitivement quelque chose..."
msgstr ""

#: game/story/home_player_noriko_room.dialogue
msgctxt "TOUT_A_EST_COMPL_TEMENT_FOU_D06408"
msgid "Tout ça est complètement fou !"
msgstr ""

#: game/story/home_player_noriko_room.dialogue
msgctxt "TU_VEUX_DIRE_QU_ILS_SONT_TOUS_8BCDD4"
msgid "Tu veux dire qu'ils sont tous corrompus ?"
msgstr ""

