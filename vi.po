msgstr ""

#: game/story/home_nobu_exterior.dialogue
msgctxt "O_EST_AKIRA_N_E57440"
msgid "OÙ EST {{akira.N}} ?!!"
msgstr ""

#: game/story/home_nobu_exterior.dialogue
msgctxt "JE_NE_SAIS_PAS_6E4D00"
msgid "JE NE SAIS PAS !!!"
msgstr ""

#: game/story/home_nobu_exterior.dialogue
msgctxt "ON_NE_TRA_NE_PAS_AVEC_LUI_CE_T_582F7F"
msgid "On ne traîne pas avec lui, ce type me fout les jetons..."
msgstr ""

#: game/story/home_nobu_exterior.dialogue
msgctxt "PEUT_TRE_QUE_KENJI_N_OU_TAKEO_9BB72D"
msgid "Peut-être que {{kenji.n}} ou {{takeo.n}} en sauront plus ?!"
msgstr ""

#: game/story/home_nobu_exterior.dialogue
msgctxt "C_EST_TOUT_CE_QUE_JE_SAIS_JE_T_AE2613"
