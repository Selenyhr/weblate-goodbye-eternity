#: game/story/home_natsume_natsume_room.dialogue
msgctxt "HAHA_BIEN_TENT_NON_C_EST_M_ME_1760C7"
msgid "Haha, bien tenté. Non, c’est même l’inverse."
msgstr ""

#: game/story/home_natsume_natsume_room.dialogue
msgctxt "DANS_MA_PREMI_RE_VIE_J_TAIS_SE_7E6EF8"
msgid "Dans ma première vie, j’étais seul. J’ai tout perdu."
msgstr ""

#: game/story/home_natsume_natsume_room.dialogue
msgctxt "C_EST_PAS_SI_DIFF_RENT_DE_MOI_9F9749"
msgid "C’est pas si différent de moi... sauf que moi, c’est ma première."
msgstr ""

#: game/story/home_natsume_natsume_room.dialogue
msgctxt "MAIS_SI_TU_NE_LA_CONNAISSAIS_P_36C988"
msgid "Mais si tu ne la connaissais pas... pourquoi t'accrocher à elle maintenant ?"
msgstr ""

#: game/story/home_natsume_natsume_room.dialogue
msgctxt "JUSTEMENT_JE_NE_L_AI_CONNUE_QU_A8F7AD"
