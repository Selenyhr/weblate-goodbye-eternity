#: game/story/home_natsume_natsume_room.dialogue
msgctxt "ALLEZ_DEBOUT_LA_MARMOTTE_961465"
msgid "Allez, debout la marmotte !"
msgstr ""

#: game/story/home_natsume_natsume_room.dialogue
msgctxt "HEIN_JE_JE_ME_SUIS_ENDORMI_42E4A7"
