msgstr ""

#: game/story/home_natsume_natsume_room.dialogue
msgctxt "POURQUOI_TRISTE_AU_CONTRAIRE_J_F10788"
msgid "Pourquoi triste ? Au contraire ! Je trouve ça mérité."
