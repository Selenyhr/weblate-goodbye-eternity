msgstr "¡Así!"

#: game/story/home_player_livingroom.dialogue game/story/home_player_yasuka_room.dialogue
msgctxt "yasuka_love_sexe_repeat_18e6d678"
msgid "Encore !"
msgstr "¡Más!"

#: game/story/home_player_livingroom.dialogue game/story/home_player_yasuka_room.dialogue
msgctxt "t9568e93c9b"
msgid "Encore !"
msgstr "¡Más!"

#: game/story/home_player_livingroom.dialogue game/story/home_player_yasuka_room.dialogue
msgctxt "yasuka_love_sexe_repeat_318c7ab4"
msgid "Ça vient, je vais jouir !"
msgstr "¡Ya viene! ¡Me voy a correr!"

#: game/story/home_player_livingroom.dialogue game/story/home_player_yasuka_room.dialogue
msgctxt "yasuka_love_sexe_repeat_8169b899"
msgid "Oh, c'est trop bon !"
msgstr "¡Oh, es tan bueno!"

#: game/story/home_player_livingroom.dialogue game/story/home_player_yasuka_room.dialogue
msgctxt "yasuka_love_sexe_repeat_047f8522"
msgid "Je tremble de partout."
msgstr "Me tiembla todo el cuerpo."

#: game/story/home_player_livingroom.dialogue game/story/home_player_yasuka_room.dialogue
msgctxt "yasuka_love_sexe_repeat_033f107b"
msgid "Tu sais que tu es incroyable..."
msgstr "Sabes, eres increíble..."

#: game/story/home_player_livingroom.dialogue game/story/home_player_yasuka_room.dialogue
