msgid "{{saito.n}} !"
msgstr "¡{{saito.n}}!"

#: game/story/home_nobu_exterior.dialogue
msgctxt "home_nobu_exterior_44784a8a"
msgid "{{saito.n}} !"
msgstr "¡{{saito.n}}!"

#: game/story/coffeebar_interior.dialogue game/story/hostess_club_exterior.dialogue
msgctxt "talk_yasuka_120e8a66"
msgid "Je t'interdis de penser ça !"
msgstr "¡No puedes pensar así!"

#: game/story/coffeebar_interior.dialogue game/story/hostess_club_exterior.dialogue
msgctxt "talk_yasuka_e9e491a3"
msgid "Je voulais que tu puisses grandir avec {{noriko.n}} normalement."
msgstr "Quería que crecieras con {{noriko.n}} normalmente."

#: game/story/coffeebar_interior.dialogue game/story/hostess_club_exterior.dialogue
msgctxt "talk_yasuka_bcbcc830"
msgid "À aucun moment, je n'aurais voulu vous imposer ça."
msgstr "Nunca hubiera querido imponerte esto."

#: game/story/coffeebar_interior.dialogue game/story/hostess_club_exterior.dialogue
msgctxt "talk_yasuka_2c652546"
msgid "Je comprends..."
msgstr "Entiendo..."

#: game/story/home_player_saito_room.dialogue
