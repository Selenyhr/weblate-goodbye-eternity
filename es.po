msgstr "Tomar una siesta"

#: game/story/home_player_saito_room.dialogue
msgctxt "t63584b8f70"
msgid "Faire une sieste"
msgstr "Tomar una siesta"

#: game/story/home_player_saito_room.dialogue
msgctxt "tbb2b1ca43a"
msgid "Faire venir {{noriko.n}} dans la chambre"
msgstr "Haz que {{noriko.n}} venga a ti"

#: game/story/home_player_saito_room.dialogue
msgctxt "sleep_menu_53c814c7"
msgid "Allez, j'ai du pain sur la planche !"
msgstr "¡Tengo trabajo por hacer!"

#: game/story/home_player_livingroom.dialogue game/story/home_player_yasuka_room.dialogue
msgctxt "yasuka_love_sexe_repeat_53c814c7"
msgid "Allez, j'ai du pain sur la planche !"
msgstr "¡Tengo mucho que hacer!"

#: game/story/home_player_saito_room.dialogue
msgctxt "t93db1deefb"
msgid "Dormir seul"
msgstr "Dormir solo"

#: game/story/home_player_saito_room.dialogue
msgctxt "td200a73aae"
msgid "Dormir avec {{asami.n}}"
msgstr "Dormir con {{asami.n}}"

#: game/story/home_player_saito_room.dialogue
msgctxt "t60c70d70b6"
msgid "Dormir avec {{noriko.n}}"
msgstr "Dormir con {{noriko.n}}"

#: game/story/home_player_saito_room.dialogue
msgctxt "t1d37f88914"
msgid "Juste un câlin."
msgstr "Solo un abrazo"

#: game/story/home_player_saito_room.dialogue
msgctxt "t9a2264af51"
msgid "Sexe avec {{yasuka.n}}"
msgstr "Sexo con {{yasuka.n}}."

