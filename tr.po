#: game/story/park.dialogue
msgctxt "park_d69b7ea7"
msgid "M-"
msgstr "B"

#: game/story/home_minato_exterior.dialogue
msgctxt "talk_minato_8f3ab287"
msgid "M-"
msgstr "B"

#: game/story/home_player_saito_room.dialogue
msgctxt "home_player_room_84154f67"
msgid "Maintenant, si tu permets, je vais aller travailler."
msgstr "Müsaadenizle ben artık kalkayım."

#: game/story/home_player_saito_room.dialogue
msgctxt "yasuka_wake_up_saito_185d010b"
msgid "[font_size=14]Mon chéri... Tu es réveillé ?[/font_size]"
msgstr "[font_size=14]Tatlım...Uyanık mısın?/font_size]"

#: game/story/home_player_saito_room.dialogue
msgctxt "yasuka_wake_up_saito_37c70972"
msgid "[font_size=14]Je peux entrer ?[/font_size]"
msgstr "[font_size=14]Girebilir miyim?[/font_size]"

#: game/story/home_player_saito_room.dialogue
msgctxt "yasuka_wake_up_saito_20d5a37e"
msgid "[font_size=14]..........[/font_size]"
msgstr "[font_size=14]Mmmh...[/font_size]"

#: game/story/home_player_saito_room.dialogue
msgctxt "yasuka_wake_up_saito_f8864334"
msgid "[font_size=14]Tant pis, j'entre quand même ![/font_size]"
msgstr "[font_size=14]Çok kötü, yine de giriyorum ![/font_size]"

#: game/story/home_player_saito_room.dialogue
msgctxt "yasuka_wake_up_saito_a6cae15e"
msgid "{{yasuka.n}}... ?"
msgstr "{{yasuka.n}}...?"

#: game/story/home_player_saito_room.dialogue
msgctxt "yasuka_wake_up_saito_8791e309"
msgid "Mais...Pourquoi tu..."
msgstr "Ama...Niye böyle"

#: game/story/home_player_saito_room.dialogue
msgctxt "yasuka_wake_up_saito_06e23992"
msgid "Tu es si distant avec moi dernièrement."
msgstr "Son zamanlarda bana çok mesafeli davrandın."

#: game/story/home_player_saito_room.dialogue
msgctxt "yasuka_wake_up_saito_bce20e5c"
msgid "Pourquoi tu ne prends pas en compte mes sentiments ?"
msgstr "Neden duygularımı dikkate almıyorsun?"

#: game/story/home_player_saito_room.dialogue
msgctxt "yasuka_wake_up_saito_039d4a89"
msgid "Quoi ? Tes sentiments ?"
msgstr "Ne? Duyguların mı?"

#: game/story/home_player_saito_room.dialogue
msgctxt "yasuka_wake_up_saito_ab0366a3"
msgid "Exactement !"
msgstr "Kesinlikle!"

#: game/story/dojo_exterior.dialogue
msgctxt "dojo_exterior_e12a02e3"
msgid "Exactement !"
msgstr "Kesinlikle!"

#: game/story/home_asami_asami_room.dialogue
msgctxt "home_asami_asami_room_0eef4f76"
msgid "Exactement !"
msgstr "Kesinlikle!"

#: game/story/home_asami_asami_room.dialogue
msgctxt "home_asami_asami_room_02896704"
msgid "Exactement !"
msgstr "Kesinlikle!"

#: game/story/home_asami_asami_room.dialogue
msgctxt "home_asami_asami_room_9dc8c52d"
msgid "Exactement !"
msgstr "Kesinlikle!"

#: game/story/hostess_club_exterior.dialogue
msgctxt "talk_thugs_dea6ba58"
msgid "Exactement !"
msgstr "Kesinlikle!"

#: game/story/home_minato_exterior.dialogue
msgctxt "home_minato_exterior_f73a5fe5"
msgid "Exactement !"
msgstr "Kesinlikle!"

#: game/story/home_player_saito_room.dialogue
msgctxt "yasuka_wake_up_saito_76b469d2"
msgid "...Mais... Tu n'aurais pas encore abusé de l'alcool ?"
msgstr "Ama...Çok mu içtin?"

#: game/story/home_player_saito_room.dialogue
msgctxt "yasuka_wake_up_saito_21e1d8e1"
msgid "Rhoo..."
msgstr "Hooo..."

#: game/story/home_player_saito_room.dialogue
msgctxt "yasuka_wake_up_saito_8c1a1799"
msgid "Tu comptes me laisser comme ça sur le pas de la porte ?"
msgstr "Beni böyle kapının önüne mi bırakacaksın?"

#: game/story/home_player_saito_room.dialogue
msgctxt "yasuka_wake_up_saito_efc6a08c"
msgid "J'ai même retrouvé un ensemble que je n'avais jamais eu l'occasion de porter."
msgstr "Daha önce hiç giyme fırsatı bulamadığım bir takım bile buldum."

#: game/story/home_player_saito_room.dialogue
msgctxt "yasuka_wake_up_saito_37d4a94a"
msgid "Tu n'as pas envie de... me l'enlever ?"
msgstr "Onu...üzerimden almak istemiyor musun?"

#: game/story/home_player_saito_room.dialogue
msgctxt "yasuka_wake_up_saito_5bb035d8"
msgid "....[w] Bon ça suffit, je te raccompagne à ta chambre, il est tard."
msgstr "....[w]Tamam, bu kadar yeter. Seni kampa geri götüreceğim. Geç oldu."

#: game/story/home_player_saito_room.dialogue
msgctxt "yasuka_wake_up_saito_bfd37731"
msgid "Rhooooo, rabat-joie !"
msgstr "Ayyyy, oyunbozanlık!"

#: game/story/home_player_saito_room.dialogue
msgctxt "yasuka_wake_up_saito_9b1e7057"
msgid "{{saito.n}} raccompagna {{yasuka.n}} jusqu'à sa chambre, elle se laissa faire."
msgstr ""
"{{saito.n}}eşlik etti ,{{yasuka.n}}'nın odasına. Hiçbir direniş göstermedi."

#: game/story/home_player_saito_room.dialogue
msgctxt "yasuka_wake_up_saito_863d32ba"
msgid "Elle semblait déçue, mais également dans un état d'ébriété qu'il n'avait jamais vu encore."
msgstr ""
"Hayal kırıklığına uğramış görünüyordu, ama aynı zamanda daha önce hiç "
"görmediği sarhoş bir durumdaydı."

#: game/story/home_player_saito_room.dialogue
msgctxt "yasuka_wake_up_saito_8d334278"
msgid "La raccompagner était la meilleure chose à faire."
msgstr "Ona eşlik etmek en iyi seçenekti."

#: game/story/home_player_saito_room.dialogue
msgctxt "yasuka_wake_up_saito_798a60bb"
msgid "Bon... La soirée a dû être compliqué pour elle visiblement."
msgstr "Öyleyse...Belli ki akşam onun için karmaşık geçmişti."

#: game/story/home_player_saito_room.dialogue
msgctxt "yasuka_wake_up_saito_32950e81"
msgid "Je verrai avec elle demain matin."
msgstr "Yarın sabah onu kontrol edeceğim."

#: game/story/home_player_saito_room.dialogue
msgctxt "yasuka_wake_up_saito_ec727f0b"
msgid "Je n'ai plus que quelques heures de sommeil, je ferais mieux de retourner me coucher."
msgstr "Sadece birkaç saatlik uykum kaldı. Şimdi yatmaya gitsem iyi olur."

#: game/story/home_player_saito_room.dialogue
msgctxt "yasuka_wake_up_saito_7679b7d4"
msgid "{{saito.n}} retourna se coucher, il retrouva vite le sommeil."
msgstr "{{saito.n}} yatağa geri döndü ve çabucak uykuya daldı."

#: game/story/home_player_saito_room.dialogue
msgctxt "yasuka_wake_up_saito_cb70a201"
msgid "La tenue sexy de {{yasuka.n}} l'ayant bien trop excité."
msgstr "{{yasuka.n}}'nın seksi kıyafeti onu çok fazla heyecanlandırmıştı."

#: game/story/home_player_saito_room.dialogue
msgctxt "yasuka_wake_up_saito_0a30d387"
msgid "Il sentit un poids sur lui l'écraser progressivement."
msgstr "Üstünde bir ağırlık hissetti, giderek eziyordu."

#: game/story/home_player_saito_room.dialogue
msgctxt "yasuka_wake_up_saito_a32beda6"
msgid "Ainsi qu'un petit courant d'air tiède souffler sur sa verge."
msgstr "Hafif bir ılık hava akımı şaftına esiyor gibiydi."

#: game/story/home_player_saito_room.dialogue
msgctxt "yasuka_wake_up_saito_7f5c845d"
msgid "Il pensait être en train de rêver..."
msgstr "Hayal gördüğünü düşündü..."

#: game/story/home_player_saito_room.dialogue
msgctxt "yasuka_wake_up_saito_137e4db1"
msgid "[font_size=14]Héhé, enfin ![/font_size]"
msgstr "[font_size=14]Hehe, nihayet ![/font_size]"

#: game/story/home_player_saito_room.dialogue
msgctxt "yasuka_wake_up_saito_05a005be"
msgid "[font_size=14]Tu es moi ![/font_size]"
msgstr "[font_size=14]Benimsin ![/font_size]"

#: game/story/home_player_saito_room.dialogue
msgctxt "yasuka_wake_up_saito_a6ab536e"
msgid "[font_size=14]J'attends ce moment depuis si longtemps ![/font_size]"
msgstr "[font_size=14]Bu anı çok uzun zamandır bekliyordum ![/font_size]"

#: game/story/home_player_saito_room.dialogue
msgctxt "yasuka_wake_up_saito_2cc6b7be"
msgid "[font_size=14]Bon appétit ![/font_size]"
msgstr "[font_size=14]Afiyet olsun![/font_size]"

#: game/story/home_player_saito_room.dialogue
msgctxt "yasuka_wake_up_saito_46277eb4"
msgid "Pourquoi je ne suis pas venu faire ça plus tôt ?"
msgstr "Neden bunu yapmak için daha önce gelmedim?"

#: game/story/home_player_saito_room.dialogue
msgctxt "yasuka_wake_up_saito_89fddab2"
msgid "C'est ça..."
msgstr "Bu kadar…"

#: game/story/home_player_saito_room.dialogue
msgctxt "yasuka_wake_up_saito_9932d178"
msgid "Continue de rêver mon chéri."
msgstr "Hayal kurmaya devam et, tatlım."

#: game/story/home_player_saito_room.dialogue
msgctxt "yasuka_wake_up_saito_91f28a95"
msgid "[i]**Mhh, j'ai l'impression que ma mâchoire va se décrocher.**[/i]"
msgstr "[i]**Mhh, çenem düşmek üzere gibi hissediyorum.**[/i]"

#: game/story/home_player_saito_room.dialogue
msgctxt "yasuka_wake_up_saito_9cd48d5c"
msgid "[i]**Qui aurait pu imaginer qu'il serait si bien membré.**[/i]"
msgstr "[i]**Bu kadar donanımlı olduğunu kim düşünürdü...**[/i]"

