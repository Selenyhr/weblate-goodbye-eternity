msgid "sakuya_66_1"
msgstr "Encontrar {{akira.n}} antes que ele escape."

#: game/story/home_nobu_exterior.dialogue
msgctxt "event_nobu_fight_at_home_2ccc1168"
msgid "Mais... Qu'est ce que tu fous allongé par terre ?"
msgstr "O que... O que diabos você está fazendo no chão?"

#: game/scripts/variables/quests.gd
msgctxt "quests.hints"
msgid "noriko_femdom_18"
msgstr "Vá ver {{noriko.n}}."

#: game/story/home_nobu_exterior.dialogue
msgctxt "event_nobu_fight_at_home_b41dbe4e"
msgid "Pfff faudrait déjà qu'un mec arrive a te supporter !"
msgstr "Pfff, um homem teria que tolerar você primeiro!"

