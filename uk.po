#: game/story/home_player_saito_room.dialogue
msgctxt "sleeping_with_yasuka_saito_room_ce44d3c0"
msgid "Encore une nuit merveilleuse..."
msgstr "Ще одна чудова ніч..."

#: game/story/home_player_saito_room.dialogue
msgctxt "sleeping_with_yasuka_saito_room_4a5609e2"
msgid "Je suis content que tu dormes aussi bien."
